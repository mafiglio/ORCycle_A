<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/NoteQuestionsScrollView"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent" >

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:background="#474334"
		    android:orientation="vertical" >

        <TextView
            android:id="@+id/tv_arsi_header_report_safety_issue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/psu_green"
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="10dp"
            android:text="@string/header_arsi_report_safety_issue"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/text_white" />

		    <RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
	            
		        <TextView
	                android:id="@+id/tv_arsi_problem_type"
	                android:layout_width="fill_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_alignParentTop="true"
	                android:text="@string/arsi_q_problem_type"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="@color/psu_sienna" />
	
	            <edu.pdx.cecs.orcycle.MultiSelectionSpinner
	                android:id="@+id/spn_arsi_problem"
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignLeft="@id/tv_arsi_problem_type"
	                android:layout_below="@id/tv_arsi_problem_type"
	                android:layout_marginBottom="6dp"
	                android:textAppearance="?android:attr/textAppearanceMedium" />
	            
	       	</RelativeLayout>
           	
		    <RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
		    		    
			    <TextView
	                android:id="@+id/tv_arsi_urgency"
	                android:layout_width="fill_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_alignParentTop="true"
	                android:text="@string/arsi_q_urgency"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="@color/psu_sienna" />

	            <Spinner
	                android:id="@+id/spn_arsi_urgency"
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignLeft="@id/tv_arsi_urgency"
	                android:layout_below="@id/tv_arsi_urgency"
	                android:layout_marginBottom="6dp"
	                android:entries="@array/arsi_a_urgency"
	                android:textAppearance="?android:attr/textAppearanceMedium" />
            
        	</RelativeLayout>

		    <RelativeLayout
	            android:id="@+id/rl_arsi_location"
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
		    		    
			    <TextView
	                android:id="@+id/tv_arsi_location"
	                android:layout_width="fill_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_alignParentTop="true"
	                android:text="@string/arsi_q_location"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="@color/psu_sienna" />

	            <Spinner
	                android:id="@+id/spn_arsi_location"
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignLeft="@id/tv_arsi_location"
	                android:layout_below="@id/tv_arsi_location"
	                android:layout_marginBottom="6dp"
	                android:entries="@array/arsi_a_location"
	                android:textAppearance="?android:attr/textAppearanceMedium" />
            
        	</RelativeLayout>
        	
			<RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >

				<TextView
				    android:id="@+id/tv_arsi_date"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:layout_alignParentLeft="true"
				    android:layout_alignParentRight="true"
				    android:layout_alignParentTop="true"
				    android:layout_marginBottom="6dp"
				    android:text="@string/arsi_date"
				    android:textAppearance="?android:attr/textAppearanceMedium"
				    android:textColor="@color/psu_sienna" />

				<Button
				    android:id="@+id/btn_issue_date"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:layout_alignParentLeft="true"
				    android:layout_alignParentRight="true"
				    android:layout_below="@+id/tv_arsi_date"
				    android:layout_marginBottom="6dp"
				    android:ems="10"
				    android:hint="@string/arsi_select_date"
				    android:paddingLeft="12dp"
				    android:textAppearance="?android:attr/textAppearanceMedium" >

				</Button>

			</RelativeLayout>

			<RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
		        <TextView
	                android:layout_width="fill_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_alignParentTop="true"
	                android:layout_marginTop="0dp"
	                android:text="@string/anq_requiredEntries"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="@color/psu_sienna" />
        	</RelativeLayout>
	        	
	        <RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
	            
				<Button
				    android:id="@+id/btn_arsi_save_report"
				    style="?android:attr/buttonBarButtonStyle"
				    android:layout_width="match_parent"
				    android:layout_height="40dp"
				    android:layout_marginLeft="2dp"
				    android:layout_marginRight="2dp"
				    android:layout_marginBottom="8dp"
				    android:background="@color/user_button_background"
				    android:paddingLeft="6dp"
				    android:paddingRight="6dp"
				    android:text="@string/aui_save"
				    android:textColor="@color/user_button_text"
				    android:textAppearance="?android:attr/textAppearanceMedium" />

        	</RelativeLayout>
        	
	        <RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
	            
        	</RelativeLayout>
	        
	    </LinearLayout>
   	    
</ScrollView>
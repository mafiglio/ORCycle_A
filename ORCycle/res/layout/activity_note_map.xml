<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".RecordTrip$DummySectionFragment" >

    <RelativeLayout
        android:id="@+id/rlNoteMapHeader"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/lcdscreen"
        android:paddingLeft="4sp"
        android:paddingRight="6sp"
        android:paddingTop="4sp" >

        <TextView
            android:id="@+id/tvHeaderFancyStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="4dp"
            android:text="@string/misc_longest_date_string"
            android:textColor="@color/text_black"
            android:textSize="15sp" >
        </TextView>
        
        <TextView
            android:id="@+id/tvHeaderSeverity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvHeaderFancyStart"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="4dp"
            android:text="@string/anmWaterFalls"
            android:textColor="@color/text_black"
            android:textSize="18sp"
            android:textStyle="bold" >

        </TextView>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <fragment
            android:id="@+id/noteMap"
            android:name="com.google.android.gms.maps.MapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:contentDescription="@string/anmNoteImageContentDescription"
            android:scaleType="centerCrop" />
        
		<ScrollView
		    android:id="@+id/report_accident_root_view"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:background="#fcfcfc"
		    android:visibility="visible" >

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:background="#474334"
		    android:orientation="vertical" >
		    		
	        <RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
		
	            <TextView
	                android:id="@+id/tv_anm_q_severity_of_problem"
	                android:layout_width="fill_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_alignParentTop="true"
	                android:layout_marginTop="0dp"
	                android:text="@string/ara_q_severity"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="#A33F1F" />
	
	            <TextView
	                android:id="@+id/tv_anm_a_severity_of_problem"
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_below="@id/tv_anm_q_severity_of_problem"
	                android:layout_marginTop="16dp"
	                android:text="@string/misc_generally_long_text_string"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="#474334" />
	            
	           	</RelativeLayout>
	            
		        <RelativeLayout
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginTop="1dp"
		            android:background="#fcfcfc"
		            android:orientation="vertical"
		            android:paddingBottom="4dp"
		            android:paddingLeft="@dimen/activity_horizontal_margin"
		            android:paddingRight="@dimen/activity_horizontal_margin"
		            android:paddingTop="12dp"
		            tools:context=".UserInfo$DummySectionFragment" >
			
       	            <TextView
       	                android:id="@+id/tv_anm_q_object"
       	                android:layout_width="match_parent"
       	                android:layout_height="wrap_content"
       	                android:layout_alignParentLeft="true"
       	                android:layout_alignParentRight="true"
       	                android:layout_alignParentTop="true"
       	                android:layout_marginTop="0dp"
       	                android:text="@string/ara_q_object"
       	                android:textAppearance="?android:attr/textAppearanceMedium"
       	                android:textColor="#A33F1F" />
		
		            <TextView
		                android:id="@+id/tv_anm_a_object"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:layout_below="@id/tv_anm_q_object"
		                android:layout_marginTop="16dp"
		                android:textColor="#474334"
		                android:text="@string/misc_generally_long_text_string"
		                android:textAppearance="?android:attr/textAppearanceMedium" />
	
				</RelativeLayout>
	
		        <RelativeLayout
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginTop="1dp"
		            android:background="#fcfcfc"
		            android:orientation="vertical"
		            android:paddingBottom="4dp"
		            android:paddingLeft="@dimen/activity_horizontal_margin"
		            android:paddingRight="@dimen/activity_horizontal_margin"
		            android:paddingTop="12dp"
		            tools:context=".UserInfo$DummySectionFragment" >
			
	   	            <TextView
	   	                android:id="@+id/tv_anm_q_actions"
	   	                android:layout_width="match_parent"
	   	                android:layout_height="wrap_content"
	   	                android:layout_alignParentLeft="true"
	   	                android:layout_alignParentRight="true"
	   	                android:layout_alignParentTop="true"
	   	                android:layout_marginTop="0dp"
	   	                android:text="@string/ara_q_actions"
	   	                android:textAppearance="?android:attr/textAppearanceMedium"
	   	                android:textColor="#A33F1F" />
		
		            <TextView
		                android:id="@+id/tv_anm_a_actions"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:layout_below="@id/tv_anm_q_actions"
		                android:layout_marginTop="16dp"
		                android:textColor="#474334"
		                android:text="@string/misc_generally_long_text_string"
		                android:textAppearance="?android:attr/textAppearanceMedium" />
	
				</RelativeLayout>
	
		        <RelativeLayout
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginTop="1dp"
		            android:background="#fcfcfc"
		            android:orientation="vertical"
		            android:paddingBottom="4dp"
		            android:paddingLeft="@dimen/activity_horizontal_margin"
		            android:paddingRight="@dimen/activity_horizontal_margin"
		            android:paddingTop="12dp"
		            tools:context=".UserInfo$DummySectionFragment" >
			
	   	            <TextView
	   	                android:id="@+id/tv_anm_q_contrib"
	   	                android:layout_width="match_parent"
	   	                android:layout_height="wrap_content"
	   	                android:layout_alignParentLeft="true"
	   	                android:layout_alignParentRight="true"
	   	                android:layout_alignParentTop="true"
	   	                android:layout_marginTop="0dp"
	   	                android:text="@string/ara_q_contributed"
	   	                android:textAppearance="?android:attr/textAppearanceMedium"
	   	                android:textColor="#A33F1F" />
		
		            <TextView
		                android:id="@+id/tv_anm_a_contrib"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:layout_below="@id/tv_anm_q_contrib"
		                android:layout_marginTop="16dp"
		                android:textColor="#474334"
		                android:text="@string/misc_generally_long_text_string"
		                android:textAppearance="?android:attr/textAppearanceMedium" />
	
				</RelativeLayout>
	
		        		        <RelativeLayout
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginTop="1dp"
		            android:background="#fcfcfc"
		            android:orientation="vertical"
		            android:paddingBottom="4dp"
		            android:paddingLeft="@dimen/activity_horizontal_margin"
		            android:paddingRight="@dimen/activity_horizontal_margin"
		            android:paddingTop="12dp"
		            tools:context=".UserInfo$DummySectionFragment" >
			
	   	            <TextView
	   	                android:id="@+id/tvNmqComment"
	   	                android:layout_width="match_parent"
	   	                android:layout_height="wrap_content"
	   	                android:layout_alignParentLeft="true"
	   	                android:layout_alignParentRight="true"
	   	                android:layout_alignParentTop="true"
	   	                android:layout_marginTop="0dp"
	   	                android:text="@string/anmComment"
	   	                android:textAppearance="?android:attr/textAppearanceMedium"
	   	                android:textColor="#A33F1F" />
		
		            <TextView
		                android:id="@+id/tvNmComment"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:layout_below="@id/tvNmqComment"
		                android:layout_marginTop="16dp"
		                android:textColor="#474334"
		                android:text="@string/misc_generally_long_text_string"
		                android:textAppearance="?android:attr/textAppearanceMedium" />
	
				</RelativeLayout>

		        <RelativeLayout
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginTop="1dp"
		            android:background="#fcfcfc"
		            android:orientation="vertical"
		            android:paddingBottom="4dp"
		            android:paddingLeft="@dimen/activity_horizontal_margin"
		            android:paddingRight="@dimen/activity_horizontal_margin"
		            android:paddingTop="12dp"
		            tools:context=".UserInfo$DummySectionFragment">
				</RelativeLayout>
		        		        
		    </LinearLayout>
		</ScrollView>
		
		<ScrollView
		    android:id="@+id/report_safety_issue_root_view"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:background="#fcfcfc"
		    android:visibility="visible" >

			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:background="#474334"
			    android:orientation="vertical" >
	        <RelativeLayout
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="1dp"
	            android:background="#fcfcfc"
	            android:orientation="vertical"
	            android:paddingBottom="4dp"
	            android:paddingLeft="@dimen/activity_horizontal_margin"
	            android:paddingRight="@dimen/activity_horizontal_margin"
	            android:paddingTop="12dp"
	            tools:context=".UserInfo$DummySectionFragment" >
		
	            <TextView
	                android:id="@+id/tv_anm_q_safety_issue"
	                android:layout_width="fill_parent"
	                android:layout_height="wrap_content"
	                android:layout_alignParentLeft="true"
	                android:layout_alignParentRight="true"
	                android:layout_alignParentTop="true"
	                android:layout_marginTop="0dp"
	                android:text="@string/arsi_q_problem_type"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="#A33F1F" />
	
	            <TextView
	                android:id="@+id/tv_anm_a_safety_issue"
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                android:layout_below="@id/tv_anm_q_safety_issue"
	                android:layout_marginTop="16dp"
	                android:text="@string/misc_generally_long_text_string"
	                android:textAppearance="?android:attr/textAppearanceMedium"
	                android:textColor="#474334" />
	            
	           	</RelativeLayout>
	            
		        <RelativeLayout
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginTop="1dp"
		            android:background="#fcfcfc"
		            android:orientation="vertical"
		            android:paddingBottom="4dp"
		            android:paddingLeft="@dimen/activity_horizontal_margin"
		            android:paddingRight="@dimen/activity_horizontal_margin"
		            android:paddingTop="12dp"
		            tools:context=".UserInfo$DummySectionFragment" >
			
       	            <TextView
       	                android:id="@+id/tv_anm_q_urgency"
       	                android:layout_width="match_parent"
       	                android:layout_height="wrap_content"
       	                android:layout_alignParentLeft="true"
       	                android:layout_alignParentRight="true"
       	                android:layout_alignParentTop="true"
       	                android:layout_marginTop="0dp"
       	                android:text="@string/ara_q_object"
       	                android:textAppearance="?android:attr/textAppearanceMedium"
       	                android:textColor="#A33F1F" />
		
		            <TextView
		                android:id="@+id/tv_anm_a_urgency"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:layout_below="@id/tv_anm_q_urgency"
		                android:layout_marginTop="16dp"
		                android:textColor="#474334"
		                android:text="@string/misc_generally_long_text_string"
		                android:textAppearance="?android:attr/textAppearanceMedium" />
	
				</RelativeLayout>
		        <RelativeLayout
		            android:layout_width="fill_parent"
		            android:layout_height="wrap_content"
		            android:layout_marginTop="1dp"
		            android:background="#fcfcfc"
		            android:orientation="vertical"
		            android:paddingBottom="4dp"
		            android:paddingLeft="@dimen/activity_horizontal_margin"
		            android:paddingRight="@dimen/activity_horizontal_margin"
		            android:paddingTop="12dp"
		            tools:context=".UserInfo$DummySectionFragment">
				</RelativeLayout>
		        	
			</LinearLayout>
		</ScrollView>
	</RelativeLayout>
</LinearLayout>